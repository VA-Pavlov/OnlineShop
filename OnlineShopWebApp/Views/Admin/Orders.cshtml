@using OnlineShopWebApp.Models.OrderClasses
@model List<Order>;
<link rel="stylesheet" href="/css/Admin/MenuStyle.css" />
<div class="sidenav">
    <a href="/Admin/Orders">Заказы</a>
    <a href="/Admin/Users">Пользователи</a>
    <a href="/Admin/Roles">Роли</a>
    <a href="/Admin/Products">Продукты</a>
</div>
@if (Model.Count > 0)
{
    <table class="table">
        @foreach (var order in Model)
        {
            <tr>
                <th>@order.Id </th>
                <th>@order.СreatingTime </th>
                <th>
                    @foreach (var product in order.OrderItems)
                    {
                        @product.Product.Name;
                        <br/>
                    }
                </th>
            </tr>
        }
    </table>
}
else
{
    <h2>Заказов пока нет</h2>
}